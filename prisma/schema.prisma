generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id      Int      @id @default(autoincrement())
  nome    String
  email   String   @unique
  carros  Carro[]  @relation("UsuariosCarros")
}

model Carro {
  id       Int       @id @default(autoincrement())
  modelo   String
  preco    Float
  usuarios Usuario[] @relation("UsuariosCarros")
}